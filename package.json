{
  "name": "ol-mapbox-gl-style",
  "version": "0.0.1",
  "description": "Use Mapbox GL style objects with OpenLayers",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/boundlessgeo/ol-mapbox-gl-style.git"
  },
  "keywords": [
    "openlayers",
    "mapbox",
    "vector tiles"
  ],
  "author": "Andreas Hocevar",
  "license": "BSD-2-Clause",
  "scripts": {
    "dist": "mkdir -p dist && browserify -p [ standalonify --name olms --deps [ null --openlayers ol ] ] index.js > dist/olms.js",
    "doc": "documentation build index.js -f md",
    "pretest": "eslint src",
    "test": "mocha"
  },
  "dependencies": {
    "color-string": "1.2.0",
    "has-font": "1.0.1",
    "mapbox-gl-function": "1.2.1"
  },
  "devDependencies": {
    "browserify": "13.1.0",
    "cssify": "1.0.3",
    "documentation": "4.0.0-beta10",
    "eslint": "3.6.1",
    "eslint-config-openlayers": "5.0.0",
    "expect.js": "0.3.1",
    "jsdom": "9.5.0",
    "jsdom-global": "2.1.0",
    "mapbox-gl-styles": "2.0.2",
    "mocha": "3.1.0",
    "openlayers": "3.18.2",
    "standalonify": "0.1.3"
  }
}
